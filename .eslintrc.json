{
  "env": {
    "browser": true,
    "jquery": true
  },
  "extends": "eslint:recommended",
  "globals": {
    "alert": false,
    "document": false,
    "$": false,
    "console": false
  },
  "parserOptions": {
    "ecmaVersion": 2015
  },
  "rules": {
    "no-extra-semi": "warn",
    "no-undef": "warn",
    "quotes": ["warn", "single"],
    "space-before-blocks": ["warn", { "functions": "always" }]
  } 
}
